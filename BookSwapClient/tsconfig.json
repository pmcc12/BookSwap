{
  "compilerOptions": {
    "strict": true,
    "experimentalDecorators": true,
    "allowJs": true,
    "esModuleInterop": true,
    "jsx": "react-native",
    "lib": ["DOM","ESNext"],
    "moduleResolution": "node",
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "target": "ESNext",
    "typeRoots": ["./types"],
  },
  "extends": "expo/tsconfig.base"
}
